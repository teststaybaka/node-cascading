{
  "compilerOptions": {
    "lib": ["dom", "es5", "es2015.collection", "es2015.promise"],
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "Node",
    "downlevelIteration": true,
    "esModuleInterop": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "incremental": true
  }
}
